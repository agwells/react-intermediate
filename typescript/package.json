{
  "name": "example-webpack-build",
  "version": "1.0.0",
  "description": "Tutorial for Webpack",
  "main": "src/index.jsx",
  "scripts": {
    "test": "./node_modules/.bin/jest",
    "test:watch": "./node_modules/.bin/jest --watchAll",
    "test:coverage": "./node_modules/.bin/jest --coverage",
    "build": "rm -rf ./dist && ./node_modules/.bin/webpack -p",
    "start": "./node_modules/.bin/webpack-dev-server --hot --inline --mode development"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jenofdoom/react-intermediate.git"
  },
  "keywords": [
    "training",
    "react",
    "intermediate"
  ],
  "author": "Jen Zajac",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/jenofdoom/react-intermediate/issues"
  },
  "homepage": "https://github.com/jenofdoom/react-intermediate#readme",
  "private": true,
  "devDependencies": {
    "@babel/core": "~7.1.6",
    "@babel/plugin-proposal-object-rest-spread": "~7.0.0",
    "@babel/polyfill": "~7.0.0",
    "@babel/preset-env": "~7.1.6",
    "@babel/preset-react": "~7.0.0",
    "@babel/preset-typescript": "^7.3.3",
    "@types/prop-types": "^15.7.1",
    "@types/react": "^16.9.2",
    "@types/react-dom": "^16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^4.3.5",
    "autoprefixer": "~9.3.1",
    "babel-core": "~7.0.0-bridge.0",
    "babel-jest": "~23.6.0",
    "babel-loader": "~8.0.4",
    "bootstrap": "~4.1.3",
    "css-loader": "~1.0.1",
    "enzyme": "~3.7.0",
    "enzyme-adapter-react-16": "~1.7.0",
    "eslint": "~5.9.0",
    "eslint-loader": "~2.1.1",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "~4.0.0-beta.0",
    "file-loader": "~2.0.0",
    "html-webpack-plugin": "~3.2.0",
    "jest": "~23.6.0",
    "node-sass": "~4.10.0",
    "postcss-flexbugs-fixes": "~4.1.0",
    "postcss-loader": "~3.0.0",
    "prop-types": "~15.6.2",
    "react": "~16.6.3",
    "react-addons-test-utils": "~15.6.2",
    "react-dom": "~16.6.3",
    "react-redux": "^5.1.1",
    "react-router-dom": "~4.3.1",
    "react-test-renderer": "~16.6.3",
    "redux": "~4.0.1",
    "redux-logger": "~3.0.6",
    "redux-thunk": "~2.3.0",
    "sass-loader": "~7.1.0",
    "style-loader": "~0.23.1",
    "ts-loader": "^6.0.4",
    "typescript": "^3.6.2",
    "url-loader": "~1.1.2",
    "webpack": "~4.26.1",
    "webpack-cli": "~3.1.2",
    "webpack-dev-server": "~3.1.10",
    "whatwg-fetch": "~3.0.0"
  },
  "jest": {
    "verbose": true,
    "moduleDirectories": [
      "src",
      "node_modules"
    ],
    "moduleNameMapper": {
      "^.+\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/jest-mock-files.js",
      "^.+\\.(scss|css)$": "<rootDir>/jest-mock-styles.js"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!node_modules/**",
      "!src/index.jsx",
      "!src/store.jsx"
    ],
    "setupFiles": [
      "./jest-setup.js"
    ]
  },
  "dependencies": {
    "@babel/plugin-transform-modules-commonjs": "~7.1.0"
  }
}
